    "eslint-config-next": "15.3.2",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "16.0.0",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-tailwindcss": "0.6.11",
